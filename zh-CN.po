msgstr "教学"

msgid "Game Design Course"
msgstr "游戏设计课程"

msgid "Programming Course"
msgstr "编程课程"

msgid "Product Management Course"
msgstr "产品管理课程"

msgid "R&D Course"
msgstr "研发课程"

msgid "Practice, Practice, Practice"
msgstr "练习，练习，练习"

msgid "G3 Pixel Cup"
msgstr "G3像素杯"

msgid "G3 Code Jam"
msgstr "G3编程大赛"

msgid "G3 Game Jam"
msgstr "G3游戏大赛"

msgid "G3 Time Trials"
msgstr "G3限时试玩"

msgid "G3 Innovation Challenge"
msgstr "G3创新挑战赛"

msgid "Book studies"
msgstr "研究书本"

